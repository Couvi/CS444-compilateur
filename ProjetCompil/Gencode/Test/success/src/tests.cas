program
	A : real;
	B : real;
	C : real;
	D : boolean; --Commentaire de test
	E : boolean;
	F : boolean;
	I : 1 .. 13;
	J : 2 .. 5;
	K : integer;
	L : array [+1 .. 18] OF integer;
	M : array [1 .. +18] OF real;
	N : array [18 ..25] of array [1.. 18] of integer; 
	O : array [-42 .. -3] of real;
	P : array [7..500] of real;
begin
	--init des var:
	A := 34.65;
	B := 3.14e+10;
	C := -34.1;
	E := false;
	I := 1;
	J := 3;
	for k := 1 to 18 do
		L[k] := k;
	end;
	for k := 1 to 18 do
		M[k] := k div 4;
	end;
	for k := 18 to 25 do
		N[k] := L;
	end;
	for k := -42 to -3 do
		O[k] := k*3/4;
	end;
	for k := 500 downto 7 do
		P[k] := k;
	end;
	--Test des affectable et des op√©rateurs
	M[6] := O[-10];
	D := not(E);
	E := D and E;
	F := E or not(D);
	
	D := (K = I);
	E := (I < A);
	F := (C > B);
	D := (B /= C);
	E := (K <= C);
	F := (B >= K);
	
	A := +B;
	K := -J;
		
	A := B + C;
	B := (I - A);
	C := A * 2;
	I := I + J;
	
	K := I div J;
	K := I mod J;
	
	A := I / J;
	A := I / B;
	A := B / J;
	A := B / C;
	
	L[I] := K;
	L[5] := K;
	M := L;
	K := L[14];
	A := L[18];
	L := N[18];
	B := N[20][8];
	K := N[J+18][8];
	
	
	-- Test des instructions
	
	while ((B <= I) and not F) or (L[5] > K) do
		B := B +1;
		L[5] := L[5] + 2;
	end;
	
	if (A < B) and (L[5] = K) then
		null;
	end;
	
	for J := I to 20 do 
      		K := J ; 
  	end ; 
	
end.
	
