program
	T : array [1 .. 4] of array [1 .. 3] of array [1 .. 2] of integer ;
	i, j : integer;
begin
	for i := 1 to 4 do 
		for j := 1 to 3 do
			write(1 * i * j," ",2 * i * j,", ");
			new_line;
			T[i][j][1] := 1;
			T[i][j][2] := 2;
		end;
    end;
    for i := 1 to 4 do 
		for j := 1 to 3 do
			write("(",T[i][j][1],", ",T[i][j][2],") ");
		end;
		new_line;
    end;

end.